{
  "name": "react-redux-async-await-boilerplate",
  "version": "0.0.3",
  "description": "A boilerplate frontend repo that uses react, redux, redux-thunk, async/await, styled-components, webpack and babel and makes as few other assumptions as possible",
  "main": "webpack.config.js",
  "author": "Matt Granmoe",
  "license": "MIT",
  "homepage": "https://github.com/granmoe/react-redux-async-await-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/granmoe/react-redux-async-await-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/granmoe/react-redux-async-await-boilerplate/issues"
  },
  "scripts": {
    "start": "better-npm-run start",
    "start:dash": "better-npm-run start:dash",
    "build": "better-npm-run build",
    "test": "jest --verbose"
  },
  "betterScripts": {
    "start": {
      "command": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "start:dash": {
      "command": "webpack-dashboard -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "keywords": [
    "react",
    "redux",
    "redux-thunk",
    "redux-little-router",
    "async-await",
    "webpack",
    "babel",
    "es6",
    "es7",
    "styled-components",
    "boilerplate",
    "template"
  ],
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-env": "0.0.8",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.28.4",
    "eslint": "^3.1.1",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^1.2.1",
    "jest": "^17.0.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.2.1",
    "react-json-tree": "^0.10.0",
    "react-redux": "^4.4.5",
    "react-router": "4.0.0-2",
    "redux": "^3.5.2",
    "redux-little-router": "^13.1.3",
    "redux-logger": "^2.6.1",
    "style-loader": "^0.18.2",
    "styled-components": "^2.1.2",
    "webpack": "^2.1.0-beta.27",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^2.1.0-beta.10",
    "whatwg-fetch": "^1.0.0"
  }
}
